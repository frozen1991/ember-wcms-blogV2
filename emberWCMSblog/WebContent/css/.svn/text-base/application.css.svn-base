* {
	margin: 0;
	padding: 0;
}

table {
	color: #605742;
}

input[type=text],input[type=password] {
	width: 140px;
	border: 1px solid #736749;
	height: 20px;
	background-color: #B1A37E;
	color: white;
}

label {
	color: #7D7257;
}

button {
	height: 25px;
	width: 80px;
}

body {
	text-align: center;
	font-size: 1em;
	font-family: Verdana, sans-serif;
}

#outerWrapper {
	background: url(../img/body.jpg) repeat-y top center;
}

#mainWrapper {
	background: url(../img/header.jpg) repeat-x;
}

#contentWrapper {
	width: 1000px;
	margin-left: auto;
	margin-right: auto;
	text-align: left;
}

#content {
	float: right;
	width: 730px;
	margin-right: 30px;
	display: inline;
}

#content-inner {
	margin-top: 20px;
	padding: 1px 0;
}

#adminNavigation {
	float: right;
	margin-top: 10px;
}

#adminNavigation ul {
	list-style: none;
	font-size: 10pt;
	background-color: #f5f2ed;
	color: #7D7257;
}

#adminNavigation ul li {
	display: block;
	position: relative;
	float: left;
}

#adminNavigation li ul {
	display: none;
	border: 2px solid #635433;
}

#adminNavigation ul li a {
	display: block;
	color: #7D7257;
	margin-left: 1px;
	text-decoration: none;
	padding: 5px 15px 5px 15px;
	white-space: nowrap
}

#adminNavigation li:hover ul {
	display: block;
	position: absolute
}

#adminNavigation li ul li:hover {
	clear: left;
	background-color: #B6AA90;
	width: 100%;
}

#adminNavigation li ul li:hover a {
	clear: left;
	color: white;
	width: 100%;
}

#footer {
	clear: both;
	border-top: 1px solid #5E534F;
	background-color: #E7E5D2;
	padding: 10px;
	color: #7D7257;
}

#loading {
	height: 19px;
	text-align: center;
}